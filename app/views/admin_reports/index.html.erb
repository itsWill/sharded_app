<p id="notice"><%= notice %></p>

<h1>Admin Reports</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_reports.each do |admin_report| %>
      <tr>
        <td><%= link_to 'Show', admin_report %></td>
        <td><%= link_to 'Edit', edit_admin_report_path(admin_report) %></td>
        <td><%= link_to 'Destroy', admin_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Report', new_admin_report_path %>
